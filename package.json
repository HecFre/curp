{
  "name": "arsus",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod && npm run scully",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "deploy": "npm run env -s && npm run build && firebase deploy --only hosting",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "compodoc": "compodoc -p tsconfig.app.json -d docs/generated -s -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate",
    "scully": "scully",
    "scully:serve": "scully serve"
  },
  "dependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@angular/service-worker": "^10.0.0",
    "@capacitor/core": "2.4.0",
    "@ionic/angular": "^5.2.2",
    "@ngx-translate/core": "^12.1.2",
    "@ngxs-labs/firestore-plugin": "^0.1.4",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@scullyio/init": "^1.0.1",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "date-fns": "^2.14.0",
    "firebase": "^7.15.4",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1000",
    "@angular-devkit/build-angular": "^0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "^10.0.0",
    "@angular/language-service": "^10.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@capacitor/android": "^2.4.0",
    "@capacitor/cli": "2.4.0",
    "@compodoc/compodoc": "^1.1.11",
    "@ngx-rocket/scripts": "^4.0.0",
    "@ngxs/schematics": "^0.0.1-alpha.5",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.14",
    "codelyzer": "^5.2.2",
    "firebase-tools": "^8.7.0",
    "fuzzy": "^0.1.3",
    "hads": "^2.1.0",
    "htmlhint": "^0.14.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.5",
    "inquirer": "^7.2.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "stylelint": "~13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}