{
  "name": "curp",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "tsc && functions-framework --target=curp --source=dist",
    "deploy": "tsc && gcloud functions deploy curp --runtime nodejs14 --trigger-http --source=dist --allow-unauthenticated --region=us-west4 --memory=8192MB --timeout=120"
  },
  "dependencies": {
    "2captcha": "^2.1.10",
    "@google-cloud/functions-framework": "^2.1.0",
    "firebase-admin": "^10.0.1",
    "firebase-functions": "^3.16.0",
    "node-fetch": "^3.1.0"
  }
}
